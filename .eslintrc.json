{
    "rules": {
      "align": [
        true,
        "parameters",
        "statements"
      ],
      "ban": false,
      "class-name": true,
      "comment-format": [
        true
      ],
      "curly": 1,
      "eofline": true,
      "forin": true,
      "no-invalid-this": 1,
      "no-unused-new": true,
      "use-isnan": 1,
      "indent": [
        true,
        "spaces"
      ],
      "interface-name": false,
      "jsdoc-format": true,
      "label-position": true,
      "max-line-length": [
        false,
        160
      ],
      "member-access": true,
      "member-ordering": [
        true,
        "static-before-instance"
      ],
      "no-any": false,
      "no-arg": true,
      "no-bitwise": 1,
      "no-conditional-assignment": true,
      "no-consecutive-blank-lines": [
        true,
        3
      ],
      "no-console": [
        true,
        "debug",
        "info",
        "time",
        "timeEnd",
        "trace"
      ],
      "no-construct": true,
      "no-parameter-properties": true,
      "no-debugger": 1,
      "no-duplicate-variable": true,
      "no-empty": 1,
      "no-eval": 1,
      "no-inferrable-types": [
        true,
        "ignore-params"
      ],
      "no-internal-module": true,
      "no-require-imports": false,
      "no-shadowed-variable": true,
      "no-string-literal": false,
      "no-switch-case-fall-through": true,
      "no-trailing-whitespace": true,
      "no-unused-expression": [
        true,
        "allow-fast-null-checks"
      ],
      "no-use-before-declare": true,
      "no-var-keyword": true,
      "no-var-requires": false,
      "object-literal-sort-keys": false,
      "linebreak-style": [
        false,
        "LF"
      ],
      "one-line": [
        true,
        "check-open-brace",
        "check-whitespace"
      ],
      "quotemark": [
        true,
        "double",
        "avoid-escape"
      ],
      "radix": 1,
      "semicolon": [
        true,
        "always",
        "ignore-interfaces",
        "ignore-bound-class-methods"
      ],
      "switch-default": true,
      "trailing-comma": [
        true,
        {
          "multiline": "never",
          "singleline": "never"
        }
      ],
      "triple-equals": [
        true,
        "allow-null-check"
      ],
      "typedef-whitespace": [
        true,
        {
          "call-signature": "nospace",
          "index-signature": "nospace",
          "parameter": "nospace",
          "property-declaration": "nospace",
          "variable-declaration": "nospace"
        },
        {
          "call-signature": "onespace",
          "index-signature": "onespace",
          "parameter": "onespace",
          "property-declaration": "onespace",
          "variable-declaration": "onespace"
        }
      ],
      "variable-name": [
        true,
        "ban-keywords"
      ],
      "whitespace": [
        true,
        "check-branch",
        "check-decl",
        "check-operator",
        "check-separator",
        "check-type"
      ]
    }
  }
  